<header class="navbar">
  <div class="navbar-inner">
    <div>
    <%= link_to 'SOCIAL BLUEPRINT', root_path, {id: "logo", class: 'brand'}%>
	<nav>
		<ul class="nav pull-right">
			<li><%= link_to "Events", 	events_path %></li>
			<li><%= link_to "Add Event", 	new_event_path %></li>
			<% if signed_in? %>
				<li><%= link_to "Import Events", ebs_path %></li>
				<li><%= link_to "Favorite Events", ev_favorites_path %></li>
				<li id="fat-menu" class="dropdown">
					<a href="#" class="dropdown-toggle" data-toggle="dropdown">
						Account <b class="caret"></b>
					</a>
					<ul class="dropdown-menu">
						<li><%= link_to 'Profile', 	current_user %></li>
						<li><%= link_to 'Settings', edit_user_path(current_user) %></li>
						<li class="divider"></li>
						<li><%= link_to 'Sign Out', signout_path, method: "delete" %></li>	
					</ul>
				</li>
			<% else %>
				<li><%= link_to "Sign In/Sign Up", 	signin_path %></li>
			<% end %>
		</ul>
	</nav>
    </div>
  </div>
</header>